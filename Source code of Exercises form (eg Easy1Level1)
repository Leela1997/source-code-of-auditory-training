Private Sub cmdResult_Click()
If cmdResult.Caption = "ÔÑæÚ ãÌÏÏ" Then
cmdPlay1.Enabled = True
cmdPlay2.Enabled = True
cmdPlay3.Enabled = True
cmdPlay4.Enabled = True
cmdPlay5.Enabled = True
cmdPlay6.Enabled = True
cmdPlay7.Enabled = True
cmdPlay8.Enabled = True
cmdPlay9.Enabled = True
cmdPlay10.Enabled = True
lblResult.Caption = ""
frmEasy1.lbl1.Caption = "0"
frmEasy1.lblRes.Caption = "Score =" + Str(Val((frmEasy1.lbl1.Caption) + Val(frmEasy1.lbl2.Caption) + Val(frmEasy1.lbl3.Caption) + Val(frmEasy1.lbl4.Caption) + Val(frmEasy1.lbl5.Caption)) / 5)
cmdResult.Caption = "ãÍÇÓÈå ÇãÊíÇÒ"
txt1.Text = ""
txt2.Text = ""
txt3.Text = ""
txt4.Text = ""
txt5.Text = ""
txt6.Text = ""
txt7.Text = ""
txt8.Text = ""
txt9.Text = ""
txt10.Text = ""
Start = 0
Else
Dim R As Integer, s As Integer
R = 0
If P1 = 1 And txt1.Text = "1" Or P1 = 2 And txt1.Text = "2" Or P1 = 3 And txt1.Text = "2" Or P1 = 4 And txt1.Text = "3" Or P1 = 5 And txt1.Text = "3" Then
R = R + 1
End If
If P2 = 1 And txt2.Text = "1" Or P2 = 2 And txt2.Text = "2" Or P2 = 3 And txt2.Text = "2" Or P2 = 4 And txt2.Text = "3" Or P2 = 5 And txt2.Text = "3" Then
R = R + 1
End If
If P3 = 1 And txt3.Text = "1" Or P3 = 2 And txt3.Text = "2" Or P3 = 3 And txt3.Text = "2" Or P3 = 4 And txt3.Text = "3" Or P3 = 5 And txt3.Text = "3" Then
R = R + 1
End If
If P4 = 1 And txt4.Text = "1" Or P4 = 2 And txt4.Text = "2" Or P4 = 3 And txt4.Text = "2" Or P4 = 4 And txt4.Text = "3" Or P4 = 5 And txt4.Text = "3" Then
R = R + 1
End If
If P5 = 1 And txt5.Text = "1" Or P5 = 2 And txt5.Text = "2" Or P5 = 3 And txt5.Text = "2" Or P5 = 4 And txt5.Text = "3" Or P5 = 5 And txt5.Text = "3" Then
R = R + 1
End If
If P6 = 1 And txt6.Text = "1" Or P6 = 2 And txt6.Text = "2" Or P6 = 3 And txt6.Text = "2" Or P6 = 4 And txt6.Text = "3" Or P6 = 5 And txt6.Text = "3" Then
R = R + 1
End If
If P7 = 1 And txt7.Text = "1" Or P7 = 2 And txt7.Text = "2" Or P7 = 3 And txt7.Text = "2" Or P7 = 4 And txt7.Text = "3" Or P7 = 5 And txt7.Text = "3" Then
R = R + 1
End If
If P8 = 1 And txt8.Text = "1" Or P8 = 2 And txt8.Text = "2" Or P8 = 3 And txt8.Text = "2" Or P8 = 4 And txt8.Text = "3" Or P8 = 5 And txt8.Text = "3" Then
R = R + 1
End If
If P9 = 1 And txt9.Text = "1" Or P9 = 2 And txt9.Text = "2" Or P9 = 3 And txt9.Text = "2" Or P9 = 4 And txt9.Text = "3" Or P9 = 5 And txt9.Text = "3" Then
R = R + 1
End If
If P10 = 1 And txt10.Text = "1" Or P10 = 2 And txt10.Text = "2" Or P10 = 3 And txt10.Text = "2" Or P10 = 4 And txt10.Text = "3" Or P10 = 5 And txt10.Text = "3" Then
R = R + 1
End If
s = (R * 100) / 10
lblResult.Caption = "Result = " & s
frmEasy1.lbl1.Caption = s
frmEasy1.lblRes.Caption = "Score =" + Str(Val((frmEasy1.lbl1.Caption) + Val(frmEasy1.lbl2.Caption) + Val(frmEasy1.lbl3.Caption) + Val(frmEasy1.lbl4.Caption) + Val(frmEasy1.lbl5.Caption)) / 5)
'If s >= 50 Then
'MsgBox ("ÔãÇ ãæÝÞ ÔÏíÏ æ ãí ÊæÇäíÏ Èå ãÑÍáå ÈÚÏ ÈÑæíÏ")
'frmEasy1.cmdLevel2.Enabled = True
'frmEasy1level1.Hide
'Else
'MsgBox ("ÇãÊíÇÒ ÔãÇ ˜ã ÇÓÊ ãÌÏÏÇ ÔÑæÚ ˜äíÏ")
'End If
If s < 80 Then
cmdResult.Caption = "ÔÑæÚ ãÌÏÏ"
Else

'frmEasy1level1.Hide
'frmEasy1level2.Show (vbModal)
End If
End If

Dim Filename As String
Dim Filenumber
Filename = "C:\Audtrain\Save\T1 (1-1-1).txt"
Filenumber = FreeFile
Open Filename For Output As Filenumber
Print #Filenumber, txt1
Print #Filenumber, txt2
Print #Filenumber, txt3
Print #Filenumber, txt4
Print #Filenumber, txt5
Print #Filenumber, txt6
Print #Filenumber, txt7
Print #Filenumber, txt8
Print #Filenumber, txt9
Print #Filenumber, txt10
Print #Filenumber, lblResult
Close #Filenumber

End Sub

Private Sub Form_Load()
Start = 0
Dim T1 As String, T2 As String, T3 As String, T4 As String, T5 As String, T6 As String, T7 As String, T8 As String, T9 As String, T10 As String, Result As String
Dim Filename As String
Dim Filenumber
Filenumber = FreeFile
Filename = "C:\Audtrain\Save\T1 (1-1-1).txt"
Open Filename For Input As Filenumber
Line Input #Filenumber, T1
Line Input #Filenumber, T2
Line Input #Filenumber, T3
Line Input #Filenumber, T4
Line Input #Filenumber, T5
Line Input #Filenumber, T6
Line Input #Filenumber, T7
Line Input #Filenumber, T8
Line Input #Filenumber, T9
Line Input #Filenumber, T10
Line Input #Filenumber, Result
Close #Filenumber
txt1 = T1
txt2 = T2
txt3 = T3
txt4 = T4
txt5 = T5
txt6 = T6
txt7 = T7
txt8 = T8
txt9 = T9
txt10 = T10
lblResult = Result
If T1 <> "" Then
cmdPlay1.Enabled = False
cmdResult.Caption = "ÔÑæÚ ãÌÏÏ"
Else
cmdPlay1.Enabled = True
End If
If T2 <> "" Then
cmdPlay2.Enabled = False
