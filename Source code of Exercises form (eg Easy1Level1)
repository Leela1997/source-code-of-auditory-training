Private Sub Timer1_Timer()
'lblRand.Caption = Left(Right(Trim(Time), 4), 1)

lblRand.Caption = Left(Right(Timer, 2), 1)

Rand = Val(lblRand.Caption)
If Rand <= 4 Then
Rand = Rand + 1
Else
Rand = Rand - 4
End If
'Label2.Caption = Str(Rand)
If cmdPlay1.Enabled = False And cmdPlay2.Enabled = False And cmdPlay3.Enabled = False And cmdPlay4.Enabled = False And cmdPlay5.Enabled = False And cmdPlay6.Enabled = False And cmdPlay7.Enabled = False And cmdPlay8.Enabled = False And cmdPlay9.Enabled = False And cmdPlay10.Enabled = False Then
cmdNext.Enabled = True
Else
cmdNext.Enabled = False
End If
End Sub


Private Sub txt1_Change()
Dim n As Integer, s As String
txt1.Text = Trim(txt1.Text)
For n = 1 To Len(txt1.Text)
s = Mid(txt1.Text, n, 1)
If Val(s) = 0 And s <> "0" Then
MsgBox ("Please insert a number")
txt1.Text = ""
Exit For
End If
Next n
End Sub

